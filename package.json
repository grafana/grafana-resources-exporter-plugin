{
  "name": "grafana-resources-exporter-app",
  "version": "0.1.0",
  "description": "Export resources from your instance or Grafana Cloud account as Terraform, Grizzly or Crossplane resource definitions.",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\""
  },
  "author": "Grafana",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-meta-extractor": "^0.0.4",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.46.0",
    "@swc/core": "^1.7.42",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.1.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.8.6",
    "@types/testing-library__jest-dom": "6.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "^8.17.1",
    "css-loader": "^7.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.83.1",
    "sass-loader": "16.0.4",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.14.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.13.0",
    "@grafana/data": "11.4.0",
    "@grafana/runtime": "11.4.0",
    "@grafana/schema": "11.4.0",
    "@grafana/ui": "11.4.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.1.3",
    "rxjs": "7.8.1",
    "tslib": "2.7.0"
  },
  "packageManager": "yarn@1.22.22"
}
